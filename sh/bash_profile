alias gb='git branch '
alias gba='git branch -a '
alias gc='git commit'
alias gca='git commit -a'
alias gd='git diff'
alias go='git checkout '
alias gp='git push'
alias gpom='git push origin master'
alias gu='git pull'
alias gf='git fetch'
